{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".it.spec.ts$",
  "testTimeout": 60000,
  "transform": {
    "^.+\\.(t)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)": "<rootDir>/../src/$1",
    "^test/(.*)": "<rootDir>/../test/$1"
  }
}